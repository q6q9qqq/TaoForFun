<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.Friendapplication" table="friendapplication">
		<id name="applicationid" type="int">
			<column name="applicationid" />
			<generator class="assigned" />
		</id>
		<property name="username1" type="java.lang.String">
			<column name="username1" />
		</property>
		<property name="username2" type="java.lang.String">
			<column name="username2" />
		</property>
	</class>
</hibernate-mapping>